{
  "name": "RenameIt",
  "description": "Keep your Sketch files organized, batch rename layers and artboards.",
  "author": "Rodrigo Soares",
  "identifier": "com.taptap.ninja.rename-it",
  "version": "4.5.3",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/rodi01/RenameIt"
  },
  "engines": {
    "sketch": ">=51.0"
  },
  "skpm": {
    "name": "Rename-It",
    "manifest": "src/manifest.json",
    "main": "Rename-It.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "format": "prettier --write '**/*.{js,jsx}'",
    "release": "standard-version"
  },
  "devDependencies": {
    "@skpm/babel-preset": "^0.2.1",
    "@skpm/builder": "^0.7.5",
    "@skpm/extract-loader": "^2.0.2",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-loader": "^0.5.5",
    "prettier": "^2.0.4",
    "standard-version": "^7.1.0",
    "svg-react-loader": "^0.4.6"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "@rodi01/renameitlib": "^1.2.3",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "sketch-module-google-analytics": "^0.2.1",
    "sketch-module-user-preferences": "^1.0.2",
    "sketch-module-web-view": "^3.4.1",
    "styled-components": "^5.1.0"
  }
}
